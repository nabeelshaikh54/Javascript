function itemsList(items)
{

console.log('List of Items in Sandwich:\n');
items.forEach(e  => {
    console.log(e);

});

}
var itemList = ['checken', 'pickles','Mayo']

itemsList(itemList);

var itemList = ['BBQ Checken', 'pickles','Mayo']

itemsList(itemList);
var itemList = ['Beaf', 'pickles','Mayo']

itemsList(itemList);
