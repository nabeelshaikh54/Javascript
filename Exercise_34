pizza = ['Pepperoni', 'BBQ', 'Cheez Pizza'];
for(i=0; i<pizza.length; i++)
{
    console.log(pizza[i]);
}
for(i=0; i<pizza.length; i++)
{
    console.log('I like '+pizza[i]+' pizza');
}
console.log('I like '+pizza[i]+' pizza');
console.log('I like thin crust pizza');
console.log('Extra cheez would make it more delicious');
console.log('10 inch pizza is enough for a lunch');
